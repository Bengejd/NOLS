{
  "name": "NOLS",
  "version": "0.4.0",
  "description": "A superb CLI tool that takes the hassle out of converting pixel (px) values to viewport values (vh/vw) with one simple command",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "build:dev": "webpack --mode=development --progress --profile --colors",
    "build:prod": "webpack --mode=production --progress --colors",
    "test": "nyc mocha --require @babel/register",
    "build-test": "npm run build:dev && npm run test",
    "build-run": "npm run build:dev && nols"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "keywords": [
    "SCSS-Converter",
    "SCSS-Translator",
    "Responsive-SCSS-Optimizer",
    "SCSS-Optimizer",
    "px-to-vh",
    "px-to-vw"
  ],
  "author": "Bengejd",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.12.9",
    "chalk": "^2.4.1",
    "inquirer": "^6.2.1",
    "line-reader": "^0.4.0"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-google": "^0.11.0",
    "http-server": "^0.11.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "terser-webpack-plugin": "^1.2.1",
    "typescript": "^3.1.6",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.0"
  },
  "bin": {
    "nols": "dist/index.js"
  }
}
